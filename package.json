{
  "name": "ant-party-server",
  "version": "0.3.8",
  "description": "Backend for Ant Party",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --recursive --require @babel/register --require regenerator-runtime tests --exit",
    "start": "node index.js",
    "dev": "./dev.sh",
    "lint": "eslint . --fix",
    "commit": "npx git-cz",
    "heroku-postbuild": "cd client && npm install && ./node_modules/.bin/eslint . && npm run build"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "jonpepler",
  "license": "GPL-3.0-only",
  "dependencies": {
    "body-parser": "^1.20.1",
    "connect-history-api-fallback": "^1.6.0",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "express": "^4.18.2",
    "express-session": "^1.17.0",
    "ioredis": "^4.16.0",
    "redis": "^3.0.2",
    "rooty": "^2.0.0",
    "socket.io": "^3.0.0",
    "uuid": "^7.0.2",
    "vm2": "^3.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.8.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.2",
    "pre-commit": "^1.2.2",
    "regenerator-runtime": "^0.13.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  }
}
